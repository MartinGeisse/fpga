
# clock
NET "clk" LOC = "C9" | IOSTANDARD = LVCMOS33;
NET "clk" PERIOD = 20.0ns HIGH 40%;

# LEDs
NET "led" LOC = "F12" | IOSTANDARD = LVTTL | SLEW = SLOW | DRIVE = 8;

# control
NET "enable"  LOC = "B4"  | IOSTANDARD = LVCMOS33  | SLEW = FAST  | DRIVE = 8;
NET "fast"  LOC = "A4"  | IOSTANDARD = LVCMOS33  | SLEW = FAST  | DRIVE = 8;
